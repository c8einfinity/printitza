<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Designnbuy\OrderTicket\Block\Link" name="customer-account-navigation-return-history-link">
                <arguments>
                    <argument name="path" xsi:type="string">orderticket/returns/history</argument>
                    <argument name="label" xsi:type="string">My Order Tickets</argument>
                    <argument name="sortOrder" xsi:type="number">10</argument>
