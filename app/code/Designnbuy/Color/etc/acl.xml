<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Designnbuy_Base::DNB">
                    <resource id="Designnbuy_Color::elements" title="Color" sortOrder="10">
                        <resource id="Designnbuy_Color::color" title="Colors" sortOrder="10" />
                        <resource id="Designnbuy_Color::category" title="Categories" sortOrder="20" />
                        <resource id="Designnbuy_Color::import_export" title="Import/Export Color" sortOrder="30" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Designnbuy_Color::config_color" title="Color Section" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
