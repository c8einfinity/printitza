						<resource id="Designnbuy_Workflow::Status" sortOrder="10" title="Status">
							<resource id="Designnbuy_Workflow::Status_save" sortOrder="10" title="Save Status"/>
							<resource id="Designnbuy_Workflow::Status_delete" sortOrder="20" title="Delete Status"/>
							<resource id="Designnbuy_Workflow::Status_update" sortOrder="30" title="Update Status"/>
							<resource id="Designnbuy_Workflow::Status_view" sortOrder="40" title="View Status"/>
						</resource>
					</resource>
				</resource>
				<resource id="Magento_Sales::sales">
					<resource id="Magento_Sales::sales_operation">
						<resource id="Designnbuy_Workflow::Reports" title="Workflow Report" translate="title" sortOrder="50" />
