							<resource id="Designnbuy_Workflow::User_save" sortOrder="10" title="Save User"/>
							<resource id="Designnbuy_Workflow::User_delete" sortOrder="20" title="Delete User"/>
							<resource id="Designnbuy_Workflow::User_update" sortOrder="30" title="Update User"/>
							<resource id="Designnbuy_Workflow::User_view" sortOrder="40" title="View User"/>
						</resource>
						<resource id="Designnbuy_Workflow::Role" sortOrder="10" title="Role">
							<resource id="Designnbuy_Workflow::Role_save" sortOrder="10" title="Save Role"/>
							<resource id="Designnbuy_Workflow::Role_delete" sortOrder="20" title="Delete Role"/>
							<resource id="Designnbuy_Workflow::Role_update" sortOrder="30" title="Update Role"/>
							<resource id="Designnbuy_Workflow::Role_view" sortOrder="40" title="View Role"/>
