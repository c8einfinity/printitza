<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <brainacts_salesrep>
            <general>
                <enabled>1</enabled>
                <manually_select>0</manually_select>
            </general>
            <dashboard>
                <withdrawal>0</withdrawal>
                <hold_period>0</hold_period>
            </dashboard>
            <admin>
                <view_only_owner>0</view_only_owner>
                <manually_assign>0</manually_assign>
                <auto_assign>0</auto_assign>
                <email_order_assigned>1</email_order_assigned>
                <template_notification>brainacts_salesrep_admin_template_notification</template_notification>
            </admin>
            <commission>
                <use_cost>0</use_cost>
                <cost_attribute>cost</cost_attribute>
            </commission>
        </brainacts_salesrep>
    </default>
</config>
